<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="account_view_account_journal_form_inherit_commissions" model="ir.ui.view">
        <field name="name">account.view.account.journal.form.inherit.commissions</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='type']" position="after">
                <field name="generates_commission"/>
            </xpath>
        </field>
    </record>

</odoo>