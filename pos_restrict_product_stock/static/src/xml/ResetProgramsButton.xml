<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="pos_restrict_product_stock.ResetProgramsButton" t-inherit="pos_loyalty.ResetProgramsButton" t-inherit-mode="extension">
        <xpath expr="//button[hasclass('control-button')]" position="replace">
            <button class="control-button btn btn-light rounded-0 fw-bolder d-none" t-att-class="{'disabled': _isDisabled()}" t-on-click="() => this.click()">
                <i class="fa fa-star me-1"></i>
                <span> </span>
                <span>Reset Programs</span>
            </button>
        </xpath>
    </t>
</templates>
